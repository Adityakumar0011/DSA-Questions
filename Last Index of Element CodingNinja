#include <bits/stdc++.h> 
#include <iostream>
using namespace std;

int main() {
    //Write your code here
    int count=-1;
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int target;
    cin>>target;
    for(int i=0;i<n;i++){
        if(arr[i]==target){
            count=i;
        }
    }
    if(count>-1){
        cout<<count;
    }
    else{
        cout<<-1;
    }

    return 0;
}
